{
    "$schema": "https://schemas.sqlpipe.com/stripe/product.json",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "object": {
            "type": "string"
        },
        "active": {
            "type": "boolean"
        },
        "created": {
            "type": "integer"
        },
        "default_price": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "images": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "additionalItems": false
        },
        "marketing_features": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "icon": {
                        "type": "string"
                    }
                },
                "additionalProperties": false
            },
            "additionalItems": false
        },
        "livemode": {
            "type": "boolean"
        },
        "metadata": {
            "type": "object",
            "properties": {
                "tier": {
                    "type": "string"
                },
                "category": {
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "name": {
            "type": "string"
        },
        "package_dimensions": {
            "type": "object",
            "properties": {
                "height": {
                    "type": "number"
                },
                "length": {
                    "type": "number"
                },
                "weight": {
                    "type": "number"
                },
                "width": {
                    "type": "number"
                }
            },
            "additionalProperties": false
        },
        "shippable": {
            "type": "boolean"
        },
        "statement_descriptor": {
            "type": "string"
        },
        "tax_code": {
            "type": "string"
        },
        "unit_label": {
            "type": "string"
        },
        "updated": {
            "type": "integer"
        },
        "url": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "name"
    ]
}